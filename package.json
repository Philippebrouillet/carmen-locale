{
  "name": "carden-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "pages:dev": "wrangler pages dev --compatibility-date=2023-10-16 --proxy 5173 -- npm run dev",
    "pages:deploy": "npm run build && wrangler pages deploy .svelte-kit/cloudflare",
    "translate": "npx @inlang/cli machine translate --project project.inlang"
  },
  "devDependencies": {
    "@inlang/paraglide-js": "1.11.1",
    "@stripe/stripe-js": "^3.5.0",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/adapter-cloudflare": "^4.4.0",
    "@sveltejs/adapter-vercel": "^6.3.0",
    "@sveltejs/kit": "^2.5.7",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.0.0",
    "lucide-svelte": "^0.399.0",
    "postcss": "^8.4.38",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.0",
    "set-cookie-parser": "^2.6.0",
    "stripe": "^16.1.0",
    "svelte": "^4.2.15",
    "svelte-check": "^3.6.0",
    "svelte-stripe": "^1.2.0",
    "svelte-tel-input": "^3.3.9",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.0.0",
    "vite": "^5.2.11"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@inlang/paraglide-sveltekit": "0.10.10",
    "@internationalized/date": "^3.5.4",
    "@macfja/svelte-persistent-store": "^2.4.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "bits-ui": "^0.21.10",
    "clsx": "^2.1.1",
    "embla-carousel-svelte": "^8.1.3",
    "mode-watcher": "^0.3.1",
    "vaul-svelte": "^0.3.1"
  }
}
